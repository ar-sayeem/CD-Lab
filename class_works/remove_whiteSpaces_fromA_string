#include <stdio.h>

int main() {
    char str[100], newStr[100];
    int i, j = 0;

    printf("Enter a string: ");
    fgets(str, sizeof(str), stdin);

    for (i = 0; str[i] != '\0'; i++) {
        if (str[i] != ' ' && str[i] != '\n') {
            newStr[j++] = str[i];
        }
    }
    newStr[j] = '\0'; // Null-terminate the new string

    printf("String after removing white spaces: %s\n", newStr);
    return 0;
}
